# Переменные для Базы Данных
DB_USER=postgres
DB_PASSWORD=your_strong_password
DB_NAME=legal_rag_db
DB_HOST=postgres_db
DB_PORT=5432
DB_URI=postgresql+asyncpg://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}

# Переменные для Qdrant (векторная база данных)
QDRANT_HOST=qdrant
QDRANT_PORT=6333
COLLECTION_NAME=legal_documents

# Переменные для Redis (кэш и хранилище сессий)
REDIS_HOST=redis
REDIS_PORT=6379

# Переменные для моделей
EMBEDDING_MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2
RERANKER_MODEL_NAME=cross-encoder/ms-marco-MiniLM-L-6-v2

# Ключи API для LLM
# Замените 'your_api_key' на ваши реальные ключи
DEEPSEEK_API_KEY=your_api_key
GEMINI_API_KEY=your_api_key

# Google Drive (если используется)
# SERVICE_ACCOUNT_FILE=path/to/your/service_account.json
# TARGET_FOLDER_ID=your_google_drive_folder_id
